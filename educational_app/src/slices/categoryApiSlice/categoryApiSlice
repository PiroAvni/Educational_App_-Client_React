import { apiSlice } from '../apiSlice/apiSlice'
const CATEGORIES_URL = 'http://localhost:5000/api/categories/'

export const categoriesApiSlice = apiSlice.injectEndpoints({
  endpoints: (builder) => ({
    categories: builder.mutation({
      query: (data) => ({
        url: `${CATEGORIES_URL}`,
        method: 'GET',
        body: data,
      }),
    }),
  }),
})

export const {
  UseCategoriesMutation,

} =categoriesApiSlice

